<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Database Interface</title>
</head>
<body>
  <div style="display: flex; flex-direction: column;">
    <div style="display: flex; flex-direction: row;">
      <div style="width: 100px; border: 10px; border-color: black; margin: 2px;">Search Word </div>
      <div style="width: auto; border: 10px; border-color: black; margin: 2px;">Results Url</div>
      </div>
      <% searches.forEach(search => { %> 
      <div style="display: flex; flex-direction: row;">
        <div style="width: 100px; border: 10px; border-color: black; margin: 2px;"><%= search.searchKey %>  </div> 
        <div style="width: auto; border: 10px; border-color: black; margin: 2px;">
          <% search.resultsUrl.split(",").forEach(link => { %> 
          <div><a href="<%= link %> "><%= link %></a> 
          </div>
          <% }) %> 
        </div>
      </div>
      <% }) %>
  </div>
</body>
</html>